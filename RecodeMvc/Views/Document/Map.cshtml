<html>
<head>
    <title> Onde tirar a documentação</title>
    <meta charset="utf-8" />
    <style>
        #map {
            height:100vh;
        }
        html, body {
            height: 100%;
            margin:0;
            padding:0;
        }
    </style>
</head>
<body>
    <div id="map"></div>
    <script>
        let map;

        function initMap() {
            map = new google.maps.Map(document.getElementById("map"), {
                center: { lat: -22.8845054100409, lng:- 43.12214924866829 },
                zoom: 300,
            });
        }
    </script>
    <script async
            src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCYezP8c7uh5hNiE8r6gKYgDUNDrQvQlZQ&callback=initMap">
    </script>
</body>
</html>